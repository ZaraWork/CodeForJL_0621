      ,I_ReserveFlag1=#IReserveFlag1#
      ,I_ReserveFlag2=#IReserveFlag2#
      ,I_ReserveFlag3=#IReserveFlag3#
      ,C_PlanCreateUser=#PlanCreateUser#
      ,C_PlanCreateTime=#PlanCreateTime#
      ,C_UpdateUser=#UpdateUser#
      ,D_UpdateTime=to_char(sysdate,'yyyyMMddhh24miss')
      where
      I_IntId = #IntId#　
      ]]>
    </update>

    <update id="UpdatePM_Pond_Bill_SuppliesStatus"
               parameterClass="PM_Pond_Bill_Supplies">
      update PM_Pond_Bill_Supplies set
      I_DataFlag=#DataFlag.EntityValue#
      ,C_PlanStatus=#PlanStatus#
      ,C_UpLoadStatus=#UpLoadStatus#
      ,I_BillStatus=#BillStatus.IntValue#
      ,C_UpdateUser=#UpdateUser#
      ,D_UpdateTime=to_char(sysdate,'yyyyMMddhh24miss')
      where
      I_IntId = #IntId#　
    </update>

    <delete id="DeletePM_Pond_Bill_Supplies" parameterClass="PM_Pond_Bill_Supplies">
      delete from PM_Pond_Bill_Supplies
      where I_IntId = #IntId#
    </delete>

