      ,a.I_WeightType
      ,a.I_TareType
      ,a.I_MoveStillType
      ,a.I_BillStatus
      ,a.D_PlanLimitTime
      ,a.C_PondLimit
      ,a.C_Remark
      ,a.C_Reserve1
      ,a.C_Reserve2
      ,a.C_Reserve3
      ,a.I_Reserve1
      ,a.I_Reserve2
      ,a.I_Reserve3
      ,a.C_CreateUser
      ,a.D_CreateTime
      ,a.C_UpdateUser
      ,a.D_UpdateTime
      from PM_Bill_Supplies a
      where a.C_WagNo =#WagNo# and a.I_BillStatus!=2  and   a.I_BillStatus!=3
      order by a.I_IntId desc
    </select>
    
    <select id="selectPM_Bill_SuppliesByCond"  resultMap="SelectPM_Bill_SuppliesAllResult" parameterClass="Hashtable">
      select
      a.I_IntId
      ,a.C_PlanNo
      ,a.C_WagNo
      ,a.C_MaterialNo
      ,a.C_MaterialName
      ,a.C_FromDeptNo
      ,a.C_FromDeptName
      ,a.C_FromStoreNo
      ,a.C_FromStoreName
      ,a.C_ToDeptNo
      ,a.C_ToDeptName
      ,a.C_ToStoreNo
      ,a.C_ToStoreName
      ,a.C_ShipNo
      ,a.C_FromStation
      ,a.C_SerialNo
      ,a.C_DeliveryNo
      ,a.C_ContractNo
      ,a.C_ProjectNo
      ,a.C_WayBillNo
      ,a.C_MarshallingNo
      ,a.I_BusinessType
      ,a.I_WeightType
      ,a.I_TareType
      ,a.I_MoveStillType
      ,a.I_BillStatus
      ,a.D_PlanLimitTime
      ,a.C_PondLimit
      ,a.C_Remark
      ,a.C_Reserve1
      ,a.C_Reserve2
      ,a.C_Reserve3
      ,a.I_Reserve1
      ,a.I_Reserve2
      ,a.I_Reserve3
      ,a.C_CreateUser
      ,a.D_CreateTime
      ,a.C_UpdateUser
      ,a.D_UpdateTime
      from PM_Bill_Supplies a
      where  a.D_CREATETIME between #StartTime# and #EndTime#
      <isNotEmpty prepend="AND" property="PlanNo">
        a.C_PlanNo=#PlanNo#
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="WagNo">
        a.C_WagNo=#WagNo#
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="BillStatus">
        a.I_BillStatus=#BillStatus#
      </isNotEmpty>
      order by a.I_IntId desc
    </select>

    <select id="selectPM_Bill_SuppliesByStatic"  resultMap="SelectPM_Bill_SuppliesAllResult" parameterClass="Hashtable">
      select
      a.I_IntId
      ,a.C_PlanNo
      ,a.C_WagNo
      ,a.C_MaterialNo
      ,a.C_MaterialName
      ,a.C_FromDeptNo
      ,a.C_FromDeptName
      ,a.C_FromStoreNo
      ,a.C_FromStoreName
      ,a.C_ToDeptNo
      ,a.C_ToDeptName
      ,a.C_ToStoreNo
      ,a.C_ToStoreName
      ,a.C_ShipNo
      ,a.C_FromStation
      ,a.C_SerialNo
      ,a.C_DeliveryNo
      ,a.C_ContractNo
      ,a.C_ProjectNo
      ,a.C_WayBillNo
      ,a.C_MarshallingNo
      ,a.I_BusinessType
      ,a.I_WeightType
      ,a.I_TareType
      ,a.I_MoveStillType
      ,a.I_BillStatus
      ,a.D_PlanLimitTime
      ,a.C_PondLimit
      ,a.C_Remark
      ,a.C_Reserve1
      ,a.C_Reserve2
      ,a.C_Reserve3
      ,a.I_Reserve1
      ,a.I_Reserve2
      ,a.I_Reserve3
      ,a.C_CreateUser
      ,a.D_CreateTime
      ,a.C_UpdateUser
      ,a.D_UpdateTime
      from PM_Bill_Supplies a
      where  a.D_CREATETIME between #StartTime# and #EndTime#
      and a.I_MoveStillType !=1 and (a.I_BillStatus = 0 or a.I_BillStatus = 1)
      order by a.I_IntId desc
    </select>
