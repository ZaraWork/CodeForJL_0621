      ,C_ContractNo=#ContractNo#
      ,C_ProjectNo=#ProjectNo#
      ,C_WayBillNo=#WayBillNo#
      ,C_MarshallingNo=#MarshallingNo#
      ,I_BusinessType=#BusinessType.IntValue#
