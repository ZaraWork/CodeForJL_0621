  <statements>
    <select id="SelectPM_OrbitWeighterBillByCondition" resultMap="SelectPM_Bill_OrbitWeighterResultMap" parameterClass="Hashtable">
      select
      T_INTID,
      T_STATUS,
      T_PRODUCT_TIME,
      T_PRODUCT_SCHEDULE,
      T_PRODUCT_MAN,
      T_PRODUCT_CLASS,
      T_REMARK,
      T_POND_ID,
      T_MARCHINE_CODE,
      T_WEIGHT_TIME,
      T_MAT_NO,
      T_THEORETICAL_WEIGHT,
      T_ACTUAL_WEIGHT,
      T_MAT_SPEC,
      T_RESERVE_V1,
      T_RESERVE_V2,
      T_RESERVE_V3,
      T_RESERVE_V4,
      T_RESERVE_V5,
      T_RESERVE_V6,
      T_RESERVE_V7,
      T_RESERVE_V8,
      T_RESERVE_V9,
      T_RESERVE_V10,
      T_BILL_STATUS
      from PM_Pond_Bill_OrbitWeighter
      where 1=1
      <isNotEmpty prepend="and" property="StartTime">
        T_WEIGHT_TIME between #StartTime# and #EndTime#
      </isNotEmpty>
      <isNotEmpty prepend="and" property="MatNo">
        T_MAT_NO=#MatNo#
      </isNotEmpty>
      order by T_WEIGHT_TIME desc
    </select>

