     </resultMap>
  </resultMaps>

  <statements>
    <select id="QueryPM_Bill_BeltByHashtable" resultMap="QueryPM_Bill_BeltByHashtableResultMap"  parameterClass="HashTable">
      select
      C_PLANSTATUS	,
      C_PLANNO	,
      C_MATERIALNO	,
      C_MATERIALNAME	,
      C_FROMDEPTNO	,
      C_FROMDEPTNAME	,
      C_FROMSTORENO	,
      C_FROMSTORENAME	,
      C_TODEPTNO	,
      C_TODEPTNAME	,
      C_TOSTORENO	,
      C_TOSTORENAME	,
      C_BELTNO	,
      C_BELTNAME	,
      C_SHIPNO	,
      C_CONTRACTNO	,
      C_VOYAGENO	,
      C_STARTTIME	,
      C_ENDTIME	,
      C_CREATETIME	,
      C_CREATENAME	,
      C_REMARK	,
      C_RESERVE1	,
      C_RESERVE2	,
      C_RESERVE3	,
      I_RESERVE1	,
      I_RESERVE2	,
      I_RESERVE3	,
      C_WGTLISTNO	,
      N_STARTWGT	,
      N_ENDWGT	,
      N_NETWGT	,
      C_MEASURESTARTTIME	,
      C_MEASUREENDTIME	,
      C_MEASUREMAN	,
      C_UPDATEUSERNAME	,
      C_UPDATETIME	,
      C_BILLCREATETIME	,
      C_BILLCREATEUSERNAME	,
      I_INTID	,
      C_UPLOADSTATUS	,
      I_BillStatus
      from PM_BILL_BELT  t
      where t.I_BillStatus!=1 
      <isNotEmpty prepend="AND" property="PlanNo">
        t.C_PLANNO=#PlanNo#
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="WgtNo">
        t.C_WGTLISTNO=#WgtNo#
      </isNotEmpty>
      <isNotEmpty prepend="AND" property="StartTime">
        <isNotEmpty  property="EndTime">
          t.C_BILLCREATETIME between #StartTime# and #EndTime#
        </isNotEmpty>
      </isNotEmpty>
      order by t.C_BILLCREATETIME desc
    </select>
    
    <select id="CreateWgtNo" resultClass="string">
       select  to_char(sysdate, 'yyyymmdd')||substr(to_char(SEQ_SERIALNUM.NEXTVAL),2,4)  from dual
    </select>
    <insert id="InsertPM_Bill_Belt" parameterClass="PM_Bill_Belt">

      <selectKey property="I_Intid" type="pre" resultClass="int">
        select SEQ_PM_BILL_BELT_ID.nextval as IntId from dual
      </selectKey>
      insert into PM_BILL_BELT
      (C_PLANSTATUS	,
      C_PLANNO	,
      C_MATERIALNO	,
      C_MATERIALNAME	,
      C_FROMDEPTNO	,
      C_FROMDEPTNAME	,
      C_FROMSTORENO	,
      C_FROMSTORENAME	,
      C_TODEPTNO	,
      C_TODEPTNAME	,
      C_TOSTORENO	,
      C_TOSTORENAME	,
      C_BELTNO	,
      C_BELTNAME	,
      C_SHIPNO	,
      C_CONTRACTNO	,
      C_VOYAGENO	,
      C_STARTTIME	,
      C_ENDTIME	,
      C_CREATETIME	,
      C_CREATENAME	,
      C_REMARK	,
      C_RESERVE1	,
      C_RESERVE2	,
      C_RESERVE3	,
      I_RESERVE1	,
      I_RESERVE2	,
      I_RESERVE3	,
      C_WGTLISTNO	,
      N_STARTWGT	,
      N_ENDWGT	,
      N_NETWGT	,
      C_MEASURESTARTTIME	,
      C_MEASUREENDTIME	,
      C_MEASUREMAN	,
      C_UPDATEUSERNAME	,
      C_UPDATETIME	,
      C_BILLCREATETIME	,
      C_BILLCREATEUSERNAME	,
      I_INTID	,
      C_UPLOADSTATUS,
      I_BillStatus
      )
      values
      ('I',
      #C_Planno#,
      #C_Materialno#,
      #C_Materialname#,
      #C_Fromdeptno#,
      #C_Fromdeptname#,
      #C_Fromstoreno#,
      #C_Fromstorename#,
      #C_Todeptno#,
      #C_Todeptname#,
      #C_Tostoreno#,
      #C_Tostorename#,
      #C_Beltno#,
      #C_Beltname#,
      #C_Shipno#,
      #C_Contractno#,
      #C_Voyageno#,
      #C_Starttime#,
      #C_Endtime#,
      #C_Createtime#,
      #C_Createname#,
      #C_Remark#,
      #C_Reserve1#,
      #C_Reserve2#,
      #C_Reserve3#,
      #I_Reserve1#,
      #I_Reserve2#,
      #I_Reserve3#,
      #C_Wgtlistno#,
      #N_Startwgt#,
      #N_Endwgt#,
      #N_Netwgt#,
      #C_Measurestarttime#,
      #C_Measureendtime#,
      #C_Measureman#,
      #C_Updateusername#,
      #C_Updatetime#,
      #C_Billcreatetime#,
      #C_Billcreateusername#,
      #I_Intid#,
      'N',
      #I_BillStatus#
      )
    </insert>
    
    <update id="InvalidPM_Bill_Belt" parameterClass="PM_Bill_Belt">
      update  PM_BILL_BELT
      set C_PLANSTATUS='D'  , I_BillStatus=1 ,  C_UPLOADSTATUS='N'  where I_INTID=#I_Intid#

    </update>
    <update id="UpdatePM_Bill_Belt" parameterClass="PM_Bill_Belt">
      update  PM_BILL_BELT set
      C_PLANSTATUS	=	#C_Planstatus#,
      C_PLANNO	=	#C_Planno#,
      C_MATERIALNO	=	#C_Materialno#,
      C_MATERIALNAME	=	#C_Materialname#,
      C_FROMDEPTNO	=	#C_Fromdeptno#,
      C_FROMDEPTNAME	=	#C_Fromdeptname#,
      C_FROMSTORENO	=	#C_Fromstoreno#,
      C_FROMSTORENAME	=	#C_Fromstorename#,
      C_TODEPTNO	=	#C_Todeptno#,
      C_TODEPTNAME	=	#C_Todeptname#,
      C_TOSTORENO	=	#C_Tostoreno#,
      C_TOSTORENAME	=	#C_Tostorename#,
      C_BELTNO	=	#C_Beltno#,
      C_BELTNAME	=	#C_Beltname#,
      C_SHIPNO	=	#C_Shipno	#,
      C_CONTRACTNO	=	#C_Contractno#,
      C_VOYAGENO	=	#C_Voyageno#,
      C_STARTTIME	=	#C_Starttime#,
      C_ENDTIME	=	#C_Endtime#,
      C_CREATETIME	=	#C_Createtime#,
      C_CREATENAME	=	#C_Createname#,
      C_REMARK	=	#C_Remark#,
      C_RESERVE1	=	#C_Reserve1#,
      C_RESERVE2	=	#	C_Reserve2#,
      C_RESERVE3	=	#C_Reserve3#,
      I_RESERVE1	=	#I_Reserve1#,
      I_RESERVE2	=	#I_Reserve2#,
      I_RESERVE3	=	#I_Reserve3#,
      C_WGTLISTNO	=	#C_Wgtlistno#,
      N_STARTWGT	=	#N_Startwgt#,
      N_ENDWGT	=	#N_Endwgt#,
      N_NETWGT	=	#N_Netwgt#,
      C_MEASURESTARTTIME	=	#C_Measurestarttime#,
      C_MEASUREENDTIME	=	#C_Measureendtime#,
      C_MEASUREMAN	=	#C_Measureman#,
      C_UPDATEUSERNAME	=	#C_Updateusername#,
      C_UPDATETIME	=	#C_Updatetime#,
      C_BILLCREATETIME	=	#C_Billcreatetime#,
      C_BILLCREATEUSERNAME	=	#C_Billcreateusername#,
      C_UPLOADSTATUS	=	'N',
      I_BillStatus=#I_BillStatus#
      where I_INTID=#I_Intid#
    </update>
  
    
  </statements>
</sqlMap>
