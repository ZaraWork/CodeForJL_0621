    <select id="selectPM_Bill_SuppliesByWgtNo"  resultMap="SelectPM_Bill_SuppliesAllResult" parameterClass="string">
      select
      a.I_IntId
      ,a.C_PlanNo
      ,a.C_WagNo
      ,a.C_MaterialNo
