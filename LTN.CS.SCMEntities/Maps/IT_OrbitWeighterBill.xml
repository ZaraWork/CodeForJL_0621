    </resultMap>

  </resultMaps>
  <statements>
    <select id="SelectIT_OrbitWeighterBillByCondition" resultMap="SelectIT_OrbitWeighterBillResultMap" parameterClass="Hashtable">
      select
      T_OPERATE_FLAG,
      T_STATUS,
      T_PRODUCT_TIME,
      T_PRODUCT_SCHEDULE,
      T_PRODUCT_CLASS,
      T_PRODUCT_MAN,
      T_REMARK,
      T_POND_ID,
      T_MARCHINE_CODE,
      T_WEIGHT_TIME,
      T_MAT_NO,
      T_THEORETICAL_WEIGHT,
      T_ACTUAL_WEIGHT,
      T_MAT_SPEC,
      T_RESERVE_V1,
      T_RESERVE_V2,
      T_RESERVE_V3,
      T_RESERVE_V4,
      T_RESERVE_V5,
      T_RESERVE_V6,
      T_RESERVE_V7,
      T_RESERVE_V8,
      T_RESERVE_V9,
      T_RESERVE_V10,
      T_UPLOAD_FLAG
      from IT_BILL_ORBITWEIGHTER
      where 1=1
      <isNotEmpty prepend="and" property="StartTime">
        T_WEIGHT_TIME between #StartTime# and #EndTime#
      </isNotEmpty>
      <isNotEmpty prepend="and" property="mat_no">
        T_MAT_NO=#mat_no#
      </isNotEmpty>
      order by T_WEIGHT_TIME desc
    </select>
  </statements>








