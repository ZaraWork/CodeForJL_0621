    </resultMap>
   </resultMaps>
  <statements>
  <select id="SelectIT_TrackScaleMaterielPlanByCondition"  resultMap="SelectIT_TrackScaleMaterielPlanResultMap" parameterClass="HashTable">
    select
    c_planstatus,
    c_planno,
    c_wagno,
    c_materialno,
    c_materialname,
    c_fromdeptno,
    c_fromdeptname,
    c_fromstoreno,
    c_fromstorename,
    c_todeptno,
    c_todeptname,
    c_tostoreno,
    c_tostorename,
    c_shipno,
    c_fromstation,
    c_serialno,
    c_deliveryno,
    c_contractno,
    c_projectno,
    c_waybillno,
    c_marshallingno,
    i_businesstype,
    i_weighttype,
    i_taretype,
    i_movestilltype,
    c_planlimittime,
    c_pondlimit,
    c_createtime,
    c_createname,
    c_remark  ,
    c_uploadstatus
    from IT_TRACKSCALEMATERIELPLAN
    where 1=1
    <isNotEmpty prepend="AND" property="PlanNo">
      C_PLANNO=#PlanNo#
    </isNotEmpty>
    <isNotEmpty prepend="AND" property="WagNo">
     C_WAGNO=#WagNo#
    </isNotEmpty>
    <isNotEmpty prepend="AND" property="StartTime">
      <isNotEmpty  property="EndTime">
        C_CREATETIME between #StartTime# and #EndTime#
      </isNotEmpty>
    </isNotEmpty>
    order by C_CREATETIME desc
  </select>
  </statements>
   
</sqlMap>
