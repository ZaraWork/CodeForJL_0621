      ,a.C_FromStoreNo
      ,a.C_FromStoreName
      ,a.C_ToDeptNo
      ,a.C_ToDeptName
      ,a.C_ToStoreNo
      ,a.C_ToStoreName
      ,a.C_ShipNo
      ,a.C_FromStation
      ,a.C_SerialNo
      ,a.C_DeliveryNo
      ,a.C_ContractNo
      ,a.C_ProjectNo
      ,a.C_WayBillNo
      ,a.C_MarshallingNo
      ,a.I_BusinessType
      ,a.I_WeightType
      ,a.I_TareType
