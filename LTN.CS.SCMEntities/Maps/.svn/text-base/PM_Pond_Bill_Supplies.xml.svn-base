    <insert id="InsertPM_Pond_Bill_Supplies"  parameterClass="PM_Pond_Bill_Supplies" >
      <selectKey property="IntId" type="pre" resultClass="int">
        select SEQ_PM_Pond_Bill_Supplies_Id.nextval as IntId from dual
      </selectKey>
      insert into PM_Pond_Bill_Supplies
      (
      I_IntId
      ,C_WgtlistNo
      ,C_PlanNo
      ,C_WagNo
      ,C_MaterialNo
      ,C_MaterialName
      ,C_FromDeptNo
      ,C_FromDeptName
      ,C_FromStoreNo
