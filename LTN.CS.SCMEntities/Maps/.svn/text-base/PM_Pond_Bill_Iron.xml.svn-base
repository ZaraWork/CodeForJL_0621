    <select id="selectPM_Pond_Bill_IronByCond"  resultMap="SelectPM_Pond_Bill_IronAllResult" parameterClass="Hashtable">
      select
      a.I_IntId
      ,a.C_WgtlistNo
      ,a.C_PlanNo
      ,a.C_HeatNo
      ,a.C_BatchNo
      ,a.C_LronNo
      ,a.C_TankNo
      ,a.C_ToDeptNo
      ,a.C_ToDeptName
      ,a.N_GrossWgt
      ,a.N_TareWgt
      ,a.N_TareWgt1
      ,a.N_NetWgt
      ,a.N_NetWgt1
      ,a.N_SlagNum
      ,a.N_NetWgt2
      ,a.D_GrossWgtTime
      ,a.D_TareWgtTime
      ,a.C_GrossWgtSiteNo
