      ,a.C_TrainGroupTare
      ,a.C_PlanStatus
      ,a.C_UpLoadStatus
      ,a.C_Remark
      ,a.C_PondRemark
      ,a.I_DataFlag
      ,a.I_BillStatus
      ,a.C_Reserve1
      ,a.C_Reserve2
      ,a.C_Reserve3
      ,a.I_Reserve1
      ,a.I_Reserve2
      ,a.I_Reserve3
      ,a.C_ReserveFlag1
      ,a.C_ReserveFlag2
      ,a.C_ReserveFlag3
      ,a.I_ReserveFlag1
      ,a.I_ReserveFlag2
      ,a.I_ReserveFlag3
      ,a.C_PlanCreateUser
      ,a.D_PlanCreateTime
      ,a.C_CreateUser
      ,a.D_CreateTime
      ,a.C_UpdateUser
      ,a.D_UpdateTime
      from PM_Pond_Bill_Iron a
      <dynamic prepend="where">
        <isParameterPresent>
          <isNotNull prepend="AND" property="TankNo" >
            a.C_TankNo = '$TankNo$'
          </isNotNull>
          <isNotNull prepend="AND" property="FormationTag" >
            a.C_TrainGroupTare = '$FormationTag$'
          </isNotNull>
          <isNotNull prepend="AND" property="BeginDate" >
            <isNotNull  property="EndDate">
              a.D_CreateTime > '$BeginDate$' and '$EndDate$'>a.D_CreateTime
            </isNotNull>
          </isNotNull>
          and a.I_DataFlag = 1 and a.I_BillStatus = 1
        </isParameterPresent>
        order by I_IntId Desc
      </dynamic>
    </select>

